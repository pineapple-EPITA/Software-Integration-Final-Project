{
  "name": "Software-Integration-Final-Project",
  "version": "1.0.0",
  "description": "Software Integration Final Project",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "mkdir -p dist && cp src/server.js dist/",
    "build:ts": "tsc --skipLibCheck",
    "lint": "eslint --ext .js,.ts --ignore-pattern \"src/__tests__/**\" src/",
    "lint:all": "eslint --ext .js,.ts .",
    "format": "prettier --write .",
    "start:dev": "NODE_ENV=dev ts-node src/boot/setup.ts",
    "start:release": "NODE_ENV=release ts-node src/boot/setup.ts",
    "start:pprod": "NODE_ENV=pprod ts-node src/boot/setup.ts",
    "start:prod": "NODE_ENV=prod ts-node src/boot/setup.ts",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:coverage": "jest --coverage --config jest.config.cjs",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.15.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.26.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.46",
    "@types/pg": "^8.15.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.4",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.5.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.32.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
